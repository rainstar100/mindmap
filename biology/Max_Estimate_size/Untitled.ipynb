    "from pymol import cmd\n",
    "import pandas as pd \n",
    "cmd.load('test.pdb','test')\n",
    "xyz=cmd.get_extent()\n",
    "df=pd.DataFrame(xyz,columns=['x','y','z'])\n",
    "size=(df.max()-df.min()).max()\n",
    "print('the max estimated size --- {:.0f} ANG'.format(size))"
